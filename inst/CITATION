citHeader("To cite clustmle in publications use:")

year <- format(Sys.Date(), "%Y")
note <- sprintf("R package version %s", meta$Version)

bibentry(
    bibtype = "Manual",
    title = "{clustmle}: Targeted Learning for clustered data",
    author = c(
        person("Maxwell", "Murphy"),
        person("Rachael", "Phillips")
    ),
    year = year,
    note = note,
    doi = "",
)
        

